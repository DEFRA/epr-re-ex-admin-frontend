{
  "name": "epr-re-ex-admin-frontend",
  "version": "0.0.0",
  "description": "Admin UI for the Re-Ex packaging Extended Producer Responsibilities service (pEPR)",
  "sideEffects": false,
  "main": "src/index.js",
  "type": "module",
  "imports": {
    "#config/*": "./src/config/*",
    "#server/*": "./src/server/*"
  },
  "engines": {
    "node": "22.20.0"
  },
  "scripts": {
    "schema:compile": "ajv compile --all-errors --verbose --data -c ajv-formats -s ./src/server/common/schemas/organisation.json -o ./src/server/common/schemas/organisation.ajv.js --code-es5",
    "build:frontend": "NODE_ENV=production webpack",
    "dev": "run-p frontend:watch server:watch",
    "dev:debug": "run-p frontend:watch server:debug",
    "docker:dev": "npm run dev",
    "format": "prettier --write \"src/**/*.js\" \"**/*.{js,cjs,md,json,config.js,test.js}\"",
    "format:check": "prettier --check \"src/**/*.js\" \"**/*.{js,cjs,md,json,config.js,test.js}\"",
    "frontend:watch": "NODE_ENV=development webpack --watch",
    "git:pre-commit-hook": "npm run format:check && npm run lint && npm test",
    "postinstall": "npm run setup:husky",
    "lint": "run-s lint:js lint:scss",
    "lint:js": "eslint .",
    "lint:js:fix": "eslint . --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --cache --cache-location .cache/stylelint --cache-strategy content --color --ignore-path .gitignore",
    "pretest": "npm run build:frontend",
    "test": "TZ=UTC vitest run --coverage",
    "test:watch": "TZ=UTC vitest",
    "server:watch": "nodemon --inspect=0.0.0.0 --ext js,json --legacy-watch ./src",
    "server:debug": "nodemon --inspect-brk=0.0.0.0 --ext js,json --legacy-watch ./src",
    "prestart": "npm run build:frontend",
    "start": "NODE_ENV=production node --use-strict .",
    "setup:husky": "node -e \"try { (await import('husky')).default() } catch (e) { if (e.code !== 'ERR_MODULE_NOT_FOUND') throw e }\" --input-type module"
  },
  "author": "Defra DDTS",
  "license": "OGL-UK-3.0",
  "dependencies": {
    "@defra/cdp-auditing": "0.6.0",
    "@defra/hapi-secure-context": "0.4.0",
    "@defra/hapi-tracing": "1.30.0",
    "@elastic/ecs-pino-format": "1.5.0",
    "@hapi/bell": "13.1.0",
    "@hapi/catbox-memory": "6.0.2",
    "@hapi/catbox-redis": "7.0.2",
    "@hapi/cookie": "12.0.1",
    "@hapi/hapi": "21.4.4",
    "@hapi/inert": "7.1.0",
    "@hapi/jwt": "3.2.1",
    "@hapi/scooter": "7.0.0",
    "@hapi/vision": "7.0.3",
    "@hapi/wreck": "18.1.0",
    "@hapi/yar": "11.0.3",
    "ajv": "8.17.1",
    "aws-embedded-metrics": "4.2.1",
    "blankie": "5.0.0",
    "convict": "6.2.4",
    "convict-format-with-validator": "6.2.0",
    "cssnano": "7.1.2",
    "cssnano-preset-default": "7.0.10",
    "date-fns": "4.1.0",
    "global-agent": "3.0.0",
    "govuk-frontend": "5.13.0",
    "hapi-pino": "13.0.0",
    "hapi-pulse": "3.0.1",
    "ioredis": "5.8.2",
    "jose": "6.1.2",
    "jsoneditor": "10.4.2",
    "lodash": "4.17.21",
    "nunjucks": "3.2.4",
    "openid-client": "6.8.1",
    "pino": "10.1.0",
    "pino-pretty": "13.1.3",
    "semver": "7.7.3",
    "undici": "7.16.0"
  },
  "devDependencies": {
    "@babel/preset-env": "7.28.5",
    "@vitest/coverage-istanbul": "4.0.15",
    "@vitest/coverage-v8": "4.0.15",
    "ajv-cli": "5.0.0",
    "ajv-formats": "3.0.1",
    "autoprefixer": "10.4.22",
    "babel-loader": "10.0.0",
    "cheerio": "1.1.2",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "13.0.1",
    "css-loader": "7.1.2",
    "eslint": "9.39.1",
    "husky": "9.1.7",
    "mini-css-extract-plugin": "2.9.4",
    "msw": "2.12.4",
    "neostandard": "0.12.2",
    "nodemon": "3.1.11",
    "npm-run-all": "4.1.5",
    "parse-gitignore": "2.0.0",
    "postcss-load-config": "6.0.1",
    "postcss-loader": "8.2.0",
    "prettier": "3.7.4",
    "sass": "1.95.0",
    "sass-embedded": "1.93.3",
    "sass-loader": "16.0.6",
    "source-map-loader": "5.0.0",
    "style-loader": "4.0.0",
    "stylelint": "16.26.1",
    "stylelint-config-gds": "2.0.0",
    "terser-webpack-plugin": "5.3.15",
    "vitest": "4.0.15",
    "vitest-fetch-mock": "0.4.5",
    "webpack": "5.103.0",
    "webpack-assets-manifest": "6.5.0",
    "webpack-cli": "6.0.1"
  },
  "overrides": {
    "tmp": "0.2.5",
    "validator": "13.15.23"
  }
}
